<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
<!--     <lifecycle> -->
<!--     	<phase-listener>br.com.geradorOkaeri.cloudinary.FieldDisableListener</phase-listener> -->
<!--     </lifecycle> -->
    <application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>properties.mensagens</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>properties.labels</base-name>
			<var>labels</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>properties.tooltips</base-name>
			<var>tooltips</var>
		</resource-bundle>
		<message-bundle>
			properties.geradormessages
		</message-bundle>
	</application>

</faces-config>
